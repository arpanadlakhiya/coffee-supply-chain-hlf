{
    "name": "test-network-bank${ORG}",
    "version": "1.0.0",
    "client": {
        "organization": "${ORG}",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": 1000,
                    "eventHub": 60,
                    "eventReg": 3
                },
                "orderer": 30
            },
            "options": {
                "grpc.keepalive_timeout_ms": 180000
            }
        }
    },
    "organizations": {
        "${ORG}": {
            "mspid": "${ORG-MSP}",
            "peers": [],
            "certificateAuthorities": [
                "${CA_ORG}"
            ]
        }
    },
    "peers": {},
    "orderers": {},
    "certificateAuthorities": {
        "${CA_ORG}": {
            "url": "${CA_URL}",
            "caName": "${CA_ORG}",
            "tlsCACerts": {
                "pem": "${CAPEM}"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}